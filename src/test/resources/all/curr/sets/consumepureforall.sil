method m8(S: Set[Ref], b:Ref, a:Int, c:Ref)
requires (forall s: Ref :: s in S ==> acc(s.f, write))
requires (forall q: Ref :: q in S ==> q.f == a)
requires b in S
requires acc(c.f, write)
requires c.f < 0
requires a > 0
ensures (forall r : Ref :: r in S ==> acc(r.f, write))
ensures (forall t : Ref :: t in S ==> (t.f == ((t == b) ? 2*a+1 : a)))
{
    b.f := 2*a+1
}

