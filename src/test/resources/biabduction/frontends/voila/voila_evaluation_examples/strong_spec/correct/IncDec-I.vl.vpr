      true ||
      IncDec_sk_$_action_n(r, lvl, x) ==
      old[pre_stabilize2](IncDec_state(r, lvl, x)) &&
      IncDec_sk_$_action_m(r, lvl, x) == IncDec_state(r, lvl, x) &&
      true &&
