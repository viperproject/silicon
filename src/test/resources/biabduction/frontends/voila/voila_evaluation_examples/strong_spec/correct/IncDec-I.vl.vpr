  requires acc(IncDec_interferenceContext_fp(r, lvl, x), write)
  ensures [(forall $_m: Int ::
      { ($_m in result) }
      ($_m in result) ==>
      ($_m in IncDec_interferenceSet_df($p0, r, lvl, x))),
    true]


function IncDec_interferenceReference_hf($p0: Int, r: Ref, lvl: Int, x: Ref): Int
