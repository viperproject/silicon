method nestedBranches1(a: Int, b: Int)
{
  var n:Int, c: Bool

  @obsolete()
  assume 0 < a
  @obsolete()
  assume a < 100
  @dependency()
  assume 0 < b
  @obsolete()
  assume b < 50
  @obsolete()
  assume c ==> a > 5

  if(c){
    if(a > b){
      @dependency()
      n := a - b
    }else{
      @dependency()
      n := a + b
    }
    @dependency()
    n := n - 1
  }else{
    @dependency()
    n := a + b
  }

  @testAssertion()
  assert n <= a + b
  assert c ==> n < a + b
}
