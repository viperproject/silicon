method nestedBranches1(a: Int, b: Int)
{
  var n:Int, c: Bool

  @irrelevant("Explicit")
  assume 0 < a
  @irrelevant("Explicit")
  assume a < 100
  @dependency("Explicit")
  assume 0 < b
  @irrelevant("Explicit")
  assume b < 50
  @irrelevant("Explicit")
  assume c ==> a > 5

  if(@irrelevant("PathCondition")(c)){
    if(@irrelevant("PathCondition")(a > b)){
      @dependency("SourceCode")
      n := a - b
    }else{
      @dependency("SourceCode")
      n := a + b
    }
    @dependency("SourceCode")
    n := n - 1
  }else{
    @dependency("SourceCode")
    n := a + b
  }

  @testAssertion("Explicit")
  assert n <= a + b
  assert c ==> n < a + b
}
