method branches1(a: Int, b: Int)
{
  var n:Int, c: Bool

  @dependency("Explicit")
  assume 0 < a
  @dependency("Explicit")
  assume b > 4

  @irrelevant("Explicit")
  assume a < 100
  @irrelevant("Explicit")
  assume b < 50
  @irrelevant("Explicit")
  assume c ==> a > 5

  if(@irrelevant("PathCondition")(c)){
    @dependency("SourceCode")
    n := a + 1
  }else{
    @dependency("SourceCode")
    n := b + 1
  }

  @testAssertion("Explicit")
  assert n > 1
}

method branches2(a: Int, b: Int)
