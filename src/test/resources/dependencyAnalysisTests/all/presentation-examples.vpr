field f: Int
field g: Int

method foo(x: Ref, y: Ref)

{
	@dependency()
    inhale acc(x.f)
	@dependency()
    inhale acc(y.f)
    @irrelevant()
    inhale acc(y.g)

	@dependency()
	assume 0 < x.f
    @irrelevant()
	assume x.f < 100
    @irrelevant()
	assume y.f < 100
    @irrelevant()
	assume 0 <= y.f
	@dependency()
	assume x.f < y.f

    @testAssertion()
	assert x.f / y.f <= 1
}