field f: Int
field g: Int

method foo(x: Ref, y: Ref)

{
	@dependency("Explicit")
    inhale acc(x.f)
	@dependency("Explicit")
    inhale acc(y.f)
    @irrelevant("Explicit")
    inhale acc(y.g)

	@dependency("Explicit")
	assume 0 < x.f
    @irrelevant("Explicit")
	assume x.f < 100
    @irrelevant("Explicit")
	assume y.f < 100
    @irrelevant("Explicit")
	assume 0 <= y.f
	@dependency("Explicit")
	assume x.f < y.f

    @testAssertion("Explicit")
	assert x.f / y.f <= 1
}