field f: Int

method divBy0(a: Ref, n: Int)
    requires @dependency("Explicit")(n > 0)
    requires @dependency("Explicit")(acc(a.f))
    requires @irrelevant("Explicit")(a.f > 0)
{
    var res: Int
    @testAssertion("Implicit")
    res := a.f / n
}

method assumeFalse()
{
    var a: Int
    @dependency("Explicit")
    assume false
    @testAssertion("Explicit")
    assert a == 2
}