
method branch1(){
    var x: Int, y: Int

    if(@dependency()(x > 0)){
        @dependency()
        y := x + 1
    }else{
        @dependency()
        y := -x + 1
    }

    @testAssertion()
    assert y > 0
}

method branch2(){
    var x: Int, y: Int

    if(@irrelevant()(x > 0)){
        @dependency()
        assume y > 0
    }else{
        @dependency()
        assume y >= 10
    }

    @testAssertion()
    assert y > 0
}

method branch3(){
    var x: Int, y: Int

    if(@dependency()(x > 0)){
        @dependency()
        y := x + 1
    }else{
        @irrelevant()
        assume y >= 10
    }

    @testAssertion()
    assert x > 0 ==> y > 0
}

method branch4(){
    var x: Int, y: Int

    if(@dependency()(x > 0)){
        @irrelevant()
        y := x + 1
    }else{
        @dependency()
        y := 10 - x
        @testAssertion()
        assert y > 0
    }
}

method branch5(){
    var x: Int, y: Int
    @dependency()
    assume y > 0

    if(@irrelevant()(x > 0)){
        @irrelevant()
        y := x + 1
    }else{
        @testAssertion()
        assert y >= 0
    }
}