field f: Int 

method branch1(){
    var x: Int, y: Int

    if(@dependency("PathCondition")(x > 0)){
        @dependency("Implicit")
        y := x + 1
    }else{
        @dependency("Implicit")
        y := -x + 1
    }

    // $PrecisionTest: $READ_WRITE=x $READ_ONLY=y $INVARIANT=y>0

    @testAssertion("Explicit")
    assert y > 0
}

method branch2(){
    var x: Int, y: Int

    if(@irrelevant("PathCondition")(x > 0)){
        @dependency("Explicit")
        assume y > 0
    }else{
        @dependency("Explicit")
        assume y >= 10
    }

    // $PrecisionTest: $READ_WRITE=x $READ_ONLY=y $INVARIANT=y>0

    @testAssertion("Explicit")
    assert y > 0
}

method branch3(){
    var x: Int, y: Int

    // $PrecisionTest: $READ_WRITE=y $READ_ONLY=x

    if(@dependency("PathCondition")(x > 10)){
        @dependency("Implicit")
        y := x + 1
    }else{
        @irrelevant("Explicit")
        assume y >= 10
    }

    @testAssertion("Explicit") $INVARIANT=y>0
    assert x > 10 ==> y > 0
}

method branch4(){
    var x: Int, y: Int

    if(@dependency("PathCondition")(x > 0)){
        @irrelevant("Implicit")
        y := x + 1
        // $PrecisionTest: $READ_WRITE=x,y $INVARIANT=x>0
    }else{
        @dependency("Implicit")
        y := 10 - x
        @testAssertion("Explicit")
        assert y > 0
    }
}

method branch5(){
    var x: Int, y: Int
    @dependency("Explicit")
    assume y > 0

    // $PrecisionTest: $READ_ONLY=x,y $INVARIANT=y>0

    if(@irrelevant("PathCondition")(x > 0)){
        @irrelevant("Implicit")
        y := x + 1
    }else{
        @testAssertion("Explicit")
        assert y >= 0
    }
}