
function sum(a: Int, b: Int): Int
    ensures result == a + b

function rand(): Int
    ensures result > 0

function func(a: Int, b: Int): Int
    requires a > 0
    ensures result >= a && result >= b && result > 0
{
    a>b? a : b
}

function useless(c: Bool): Int
    requires c
    ensures result == 5

method funcClient(a: Int, b: Int)
    requires a > 0
{
    var res: Int
    res := func(a, b)
    assert res >= 0
    assert rand() + rand() > 0
}