
predicate greater0(n: Int)
{
  n > 0
}

method foo(n: Int, b: Bool)
  requires b ==> greater0(n) // this is not reported in any unsat core!
  // requires n > 0
{
    var x: Int
    if(b){
      unfold greater0(n)
      x := n + 1
      fold greater0(n)
    }else{
      x := 19
    }
    
    assert x > 1
}