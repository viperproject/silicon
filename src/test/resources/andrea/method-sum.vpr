method sumClient(x: Int, y: Int)
{
  assume x >= 0
  assume y >= 0
  assume x < y
  var n: Int := sum(x, y)
  var n2: Int := sum(x/y, y)
  n := n + n2

  assert n >= x + 2*y
  assert n >= 0
}