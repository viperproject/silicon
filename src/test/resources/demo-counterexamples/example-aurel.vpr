field x: Int
field y: Int
predicate StructA(this: Ref) {
  acc(this.x) && acc(this.y)
}
predicate StructB(this: Ref) {
  acc(this.x) && acc(this.y)
}
method foo(a: Ref, b: Ref)
  requires(StructA(a))
  requires(StructB(b))
{
  unfold StructA(a)
  unfold StructB(b)
  a.x := 3
  a.y := 4
  b.x := 5
  b.y := 6
  assert(false)
}

